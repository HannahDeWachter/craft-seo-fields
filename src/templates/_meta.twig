{# SEO Fields meta fields start #}
    <title>{{ meta.getPageTitle(entry) }}</title>

    <meta name="description" content="{{ meta.metaDescription }}">
    <meta property="og:url" content="{{ entry.getUrl() }}" />
    <meta property="og:type" content="website" />
    <meta property="og:title" content="{{ meta.getOgTitle(entry) }}" />
{% if meta.getOgImage() %}
    <meta property="og:image" content="{{ meta.getOgImage().getUrl() }}" />
    <meta property="og:image:width" content="{{ meta.getOgImage().getWidth() }}" />
    <meta property="og:image:height" content="{{ meta.getOgImage().getHeight() }}" />
{% endif %}
    <meta property="og:description" content="{{ meta.getOgDescription() }}" />
    <meta property="og:site_name" content="" />
    <meta property="og:locale" content="{{ currentSite.language }}" />
{# SEO Fields meta fields end #}