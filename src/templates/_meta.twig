{# SEO Fields meta fields start #}
{% if meta %}

    <title>{{ meta.getPageTitle(entry) }}</title>

    <meta name="description" content="{{ meta.metaDescription }}">
    <meta property="og:url" content="{{ entry.getUrl() }}"/>
    <meta property="og:type" content="website"/>
    <meta property="og:title" content="{{ meta.getOgTitle(entry) }}"/>
    {% if meta.getOgImage() %}
        <meta property="og:image" content="{{ meta.getOgImage().url }}"/>
        <meta property="og:image:width" content="{{ meta.getOgImage().width }}"/>
        <meta property="og:image:height" content="{{ meta.getOgImage().height }}"/>
    {% endif %}
    <meta property="og:description" content="{{ meta.getOgDescription() }}"/>
    <meta property="og:site_name" content=""/>
    <meta property="og:locale" content="{{ currentSite.language }}"/>

    {# Twitter #}
    <meta name="twitter:card" content="summary_large_image" />
    <meta name="twitter:site" content="" />
    <meta name="twitter:url" content="{{ craft.app.request.url }}" />
    <meta name="twitter:title" content="{{ meta.getTwitterTitle(entry) }}" />
    <meta name="twitter:description" content="{{ meta.getTwitterDescription() }}" />
    <meta name="twitter:image" content="{{ meta.getTwitterImage().url }}" />
{% endif %}
{# SEO Fields meta fields end #}